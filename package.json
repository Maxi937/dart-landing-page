    "hapi-react-views": "^11.0.0",
    "hapi-swagger": "^16.0.1",
    "http-errors": "~1.6.3",
    "joi": "^17.8.4",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "lowdb": "^1.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.7.2",
    "node-fetch": "^3.3.1",
    "strip-ansi": "^7.1.0",
    "uuid": "^8.3.2",
